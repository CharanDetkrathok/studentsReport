<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SigninMain</servlet-name>
        <servlet-class>student.signin.controllers.SigninMain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainIndex</servlet-name>
        <servlet-class>student.main.controllers.MainIndex</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignoutMain</servlet-name>
        <servlet-class>student.signin.controllers.SignoutMain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportCenral</servlet-name>
        <servlet-class>student.main.controllers.ReportCenral</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportProvincial</servlet-name>
        <servlet-class>student.main.controllers.ReportProvincial</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportCentralDetail</servlet-name>
        <servlet-class>student.main.controllers.ReportCentralDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportProvincialDetail</servlet-name>
        <servlet-class>student.main.controllers.ReportProvincialDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterMain</servlet-name>
        <servlet-class>student.signin.controllers.RegisterMain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportCentralDetailReport</servlet-name>
        <servlet-class>student.main.controllers.ReportCentralDetailReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportCenralExportExcel</servlet-name>
        <servlet-class>student.main.controllers.ReportCenralExportExcel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportProvincialExportExcel</servlet-name>
        <servlet-class>student.main.controllers.ReportProvincialExportExcel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportProvincialDetailReport</servlet-name>
        <servlet-class>student.main.controllers.ReportProvincialDetailReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SigninMain</servlet-name>
        <url-pattern>/SigninMain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainIndex</servlet-name>
        <url-pattern>/MainIndex</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignoutMain</servlet-name>
        <url-pattern>/SignoutMain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportCenral</servlet-name>
        <url-pattern>/ReportCenral</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportProvincial</servlet-name>
        <url-pattern>/ReportProvincial</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportCentralDetail</servlet-name>
        <url-pattern>/ReportCentralDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportProvincialDetail</servlet-name>
        <url-pattern>/ReportProvincialDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterMain</servlet-name>
        <url-pattern>/RegisterMain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportCentralDetailReport</servlet-name>
        <url-pattern>/ReportCentralDetailReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportCenralExportExcel</servlet-name>
        <url-pattern>/ReportCenralExportExcel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportProvincialExportExcel</servlet-name>
        <url-pattern>/ReportProvincialExportExcel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportProvincialDetailReport</servlet-name>
        <url-pattern>/ReportProvincialDetailReport</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
